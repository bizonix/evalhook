# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

# PHP PSR-2 Coding Standards
# http://www.php-fig.org/psr/psr-2/

root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{php, c, cpp, cc}]
charset = utf-8
trim_trailing_whitespace = true
indent_style = space
indent_size = 4
